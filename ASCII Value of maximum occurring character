//ASCII Value of maximum occurring character

#include <stdio.h>
#iclude <string.h>
int main()
{
    cha str[100];
    int frequency[256]={0};
    
    printf("Ener a string:");
    scanf("%s",str);
    
    for (int i = 0;i < strlen(str);i++)
    {
        frequency[(int)str[i]] ==;
    }
    
    int maxAscii==-1;
    for(int i=0;i<256;i++)
    {
       if (frequency[i]%2==1 && frequency[i]>0)
     {
     if (maxAscii==-1|| i>maxAscii)
     {
         maxAscii=i;
      }
     }
    }
    if(maxAscii!=-1)
    {printf("%d\n");//no character with odd frequency
    }return 0;
}
